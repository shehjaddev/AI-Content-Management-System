# Root ignore rules for ai-content-management-system

# Dependencies
/node_modules
/backend/node_modules
/frontend/node_modules

# Backend build & env
/backend/dist
/backend/.env

# Frontend tooling & build
/frontend/.pnp
/frontend/.pnp.*
/frontend/.yarn/*
!/frontend/.yarn/patches
!/frontend/.yarn/plugins
!/frontend/.yarn/releases
!/frontend/.yarn/versions
/frontend/coverage
/frontend/.next/
/frontend/out/
/frontend/build
/frontend/.vercel
/frontend/*.tsbuildinfo
/frontend/next-env.d.ts

# Misc & OS files
.DS_Store
*.pem

# Debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# Env files (global)
.env*
